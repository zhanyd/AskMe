<view class="body">
<view class="title"><text>问题列表</text></view>

<view class="list">
  <view class="item" wx:for="{{questionList}}" wx:key="*this" data-id="{{item.id}}" bindtap="godetail">
    {{index+1}}: {{item.title}}
  </view>
</view>

<view class="btn-area">
    <button type="primary" bindtap="goAddQuestion" style="width:30%;" >我要提问</button>
</view>

</view>